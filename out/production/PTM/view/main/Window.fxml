<BorderPane prefHeight="600" prefWidth="900" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.WindowController">
  
   <left>
   <MyViewList fx:id="viewlist"></MyViewList>
   </left>
    <top>
     <OpenFiles fx:id="openFiles"></OpenFiles>
   </top>
